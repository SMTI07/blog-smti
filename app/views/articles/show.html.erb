<%provide(:page_title, @article.title) %>
<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @article.content.html_safe %>
</p>



<%= link_to 'Edit', edit_article_path(@article), class:'btn btn-primary btn-a-white' %> |
<%= link_to 'Back', articles_path, class:'btn btn-danger btn-a-white material-icon' %>
