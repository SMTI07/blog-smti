<%= simple_form_for(@photo) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :user_id %>
    <%= f.input :image %>
    <%= f.association :album %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
